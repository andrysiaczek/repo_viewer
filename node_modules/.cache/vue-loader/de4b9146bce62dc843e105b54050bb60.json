     return {\n        username: null,\n        repos: null,\n        sortedRepos: null,\n        get: true,\n        loading: true,\n        error: false,\n        notFound: false,\n      }\n    },\n    watch: {\n      username() {\n        axios\n        .get('https://api.github.com/users/' + this.username + '/repos')\n        .then(res => {\n          this.repos = res.data;\n          this.notFound = false;\n          this.error = false;\n        })\n        .catch( error =>  {\n          if (error.response.status == \"404\") {\n            this.notFound = true;\n          }\n          this.error = true;\n        })\n        .finally( () => this.loading = false )\n      },\n    },\n}\n</script>\n\n<style>\n.app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]}]}