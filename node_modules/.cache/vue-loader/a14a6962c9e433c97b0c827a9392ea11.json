ror: false,\n        notFound: false,\n      }\n    },\n    watch: {\n      username() {\n        axios\n        .get('https://api.github.com/users/' + this.username + '/repos')\n        .then(res => {\n          this.repos = res.data;\n          this.notFound = false;\n          this.error = false;\n        })\n        .catch( error =>  {\n          if (error.response.status === 404) {\n            this.notFound = true;\n          }\n          this.error = true;\n        })\n        .finally( () => this.loading = false )\n      },\n    },\n}\n</script>"]}]}